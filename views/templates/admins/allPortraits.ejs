<% layout('layouts/boilerplate')%>
    <div class="container gallery_html pt-3 pb-5 mb-0 mt-0">
      <div class="row justify-content-center pt-3 pb-3">
         <div class="col text-center">
             <h2>All portraits</h2>
         </div>
     </div>

      <div class="row justify-content-center pt-3 pb-3 vh-50">
        <% for (let img of allPortraits)  {%>
        <div class="col-3 text-center pt-3 pb-3 h-100">
          <img src="<%= img.url %>" class="img-fluid rounded object-fit-fill" alt="..." data-bs-toggle="modal" data-bs-target="#<%= img.url %>">
    
        </div>
        <% } %>
      </div>
      
        <div class="row justify-content-center pt-3 pb-3">
         <div class="col text-center">
           <a href="/admin/workspace"><button class="btn btn-success">Back to workspace</button></a>
         </div>
       </div>
  
    </div>